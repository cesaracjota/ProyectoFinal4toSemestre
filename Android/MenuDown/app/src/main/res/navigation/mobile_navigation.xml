<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_favorite">

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.miempresa.menudown.ui.favorite.FavoriteFragment"
        android:label="@string/title_favorite"
        tools:layout="@layout/fragment_favorite" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.miempresa.menudown.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.miempresa.menudown.ui.search.SearchFragment"
        android:label="@string/title_vehiculos"
        tools:layout="@layout/fragment_search" />
</navigation>